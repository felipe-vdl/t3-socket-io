generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Entry {
  id        String   @id @default(cuid())
  json      Json     @db.Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("entries")
}
